{
	"name": "@kitschpatrol/tweakpane-plugin-thumbnail-list",
	"version": "1.0.1",
	"description": "A fork of tweakpane-plugin-thumbnail-list with Tweakpane 4 compatibility and externalized dependencies.",
	"main": "dist/tweakpane-plugin-thumbnail-list.js",
	"type": "module",
	"types": "dist/types/index.d.ts",
	"author": "Don McCurdy <dm@donmccurdy.com>",
	"license": "MIT",
	"browser": {
		"tweakpane-plugin-thumbnail-list": false
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE.txt"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/tweakpane-plugin-thumbnail-list.git"
	},
	"keywords": [
		"npm-package",
		"tweakpane",
		"tweakpane-plugin"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"assets": "run-s clean build assets:version assets:zip",
		"assets:version": "node scripts/assets-append-version.js",
		"assets:zip": "zip -x '*types*' -j -r $(node scripts/dist-name.js)-$(cat package.json | npx json version).zip dist",
		"build": "run-p build:*",
		"build:dev": "rollup --config rollup.config.js",
		"build:dts": "tsc --project src/tsconfig-dts.json",
		"build:prod": "rollup --config rollup.config.js --environment BUILD:production",
		"clean": "rimraf dist *.tgz *.zip",
		"format": "run-p format:*",
		"format:scss": "prettier --parser scss --write \"src/sass/**/*.scss\"",
		"format:ts": "eslint --ext .ts --fix \"src/**/*.ts\"",
		"postversion": "git push --tags",
		"prepare": "run-s clean build",
		"prepublishOnly": "npm test",
		"server": "http-server -c-1 -o /test/browser.html",
		"start": "run-p watch server",
		"test": "eslint --ext .ts \"src/**/*.ts\"",
		"watch": "run-p watch:*",
		"watch:sass": "onchange --initial --kill \"src/sass/**/*.scss\" -- npm run build:dev",
		"watch:ts": "onchange --initial --kill \"src/**/*.ts\" -- rollup --config rollup.config.js"
	},
	"dependencies": {
		"@tweakpane/core": "^2.0.3"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-replace": "^2.4.2",
		"@rollup/plugin-typescript": "^8.5.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"http-server": "^14.1.1",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"postcss": "^8.4.38",
		"prettier": "^2.8.8",
		"rimraf": "^3.0.2",
		"rollup": "^2.79.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.77.1",
		"tweakpane": "^4.0.3",
		"typescript": "^4.9.5"
	},
	"peerDependencies": {
		"tweakpane": "^4.0.3"
	},
	"packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
